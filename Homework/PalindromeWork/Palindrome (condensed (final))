public class Main{
    public static void main(String[] args){
        System.out.printf(args[0] + "%s\n", args[0].toLowerCase().replaceAll("[^a-z]", "").equals(new StringBuilder(args[0].toLowerCase().replaceAll("[^a-z]", "")).reverse().toString()) ? " is a palindrome." : " is not a palindrome.");
    }
}
