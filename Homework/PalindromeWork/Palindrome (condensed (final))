public class Main{
    public static void main(String[] args){
        String a = args[0].toLowerCase().replaceAll("[^a-z]", "");
        StringBuilder b = new StringBuilder();
        for(int i = a.length()-1; i >= 0 ; i--)
	        b.append(a.charAt(i));
        System.out.printf(args[0] + "%s\n", a.equals(b.toString()) ? " is a palindrome." : " is not a palindrome.");}}
